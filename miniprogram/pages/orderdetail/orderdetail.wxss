.view {
  display: flex;
  height: 64px;
}

.view>image {
  width: 24px;
  height: 24px;
  background-color: transparent;
  transform: rotate(180deg);
  margin: 26px 16px 8px;
}

.main {
  display: flex;
  flex-direction: column;
  padding: 0 1rem;
  align-items: flex-start;
  margin: 1rem 0;
}

::-webkit-scrollbar {
  display: none;
}

.title {
  font-size: 30pt;
  align-self: stretch;
}

.title::after {
  display: inline-block;
  content: '';
  height: 8px;
  width: 8px;
  border: solid 2px #a16b47;
  border-radius: 8px;
  position: relative;
  bottom: 35px;
  right: -5px;
}

.wuliu {
  display: flex;
  padding: 0.5rem;
  justify-content: space-between;
}

.wuliu>text {
  padding:0 0.5rem;
  flex-grow: 1;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.address {
  display: flex;
  justify-content: space-between;
  padding: 0.5rem;
  border-bottom: solid 10px #f6f6f6;
  font-size: smaller;
}

.address>image {
  align-self: center;
  width: 30px;
  height: 30px;
  margin-right: 0.5rem;
}

.address>view {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
}

.address>view text {
  margin: 0.1rem 0;
}

.item {
  display: flex;
  margin: 0.75rem;
  justify-content: space-between;
}

.item>image {
  background-color: #f6f6f6;
  border-radius: 5pt;
}

.price {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  padding-left: 0.5rem;
  justify-content: space-around;
  font-size: smaller;
  width: calc(0.52*750rpx);
}

.price>text:first-child {
  font-size: 0.9rem;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.num {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  color: #a16b47;
  font-size: 1rem;
}

.num>text:last-child {
  font-size: smaller;
  color: silver;
  text-align: right;
}

.deliver {
  display: flex;
  justify-content: space-between;
  padding: 0.5rem 0.75rem;
  border-top: solid 1px #f6f6f6;
}

.deliver>text:last-child {
  color: #a16b47;
}

.mark {
  padding: 0.5rem 0.75rem;
  border-bottom: solid 1px #f6f6f6;
}

.total {
  display: flex;
  justify-content: space-between;
  padding: 0.5rem 0.75rem;
  color: #a16b47;
  border-bottom: solid 1px #f6f6f6;
}

.actualtotal {
  display: flex;
  justify-content: space-between;
  padding: 0.5rem 0.75rem;
  color: #a16b47;
  border-bottom: solid 10px #f6f6f6;
}

.mains {
  display: flex;
  flex-direction: column;
  margin: 0.5rem;
  border-radius: 4px;
  /* box-shadow: 0 0 1px #a16b47; */
  font-size: medium;
  background-color: white;
}

::-webkit-scrollbar {
  display: none;
}

.serinum {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

