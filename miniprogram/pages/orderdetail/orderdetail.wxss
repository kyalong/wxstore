.serinum>text:nth-child(2) {
  margin-left: 0.5rem;
  flex-grow: 1;
}

.serinum>text:last-child {
  color: transparent;
  margin-right: 0.5rem;
  background-image: linear-gradient(13.5deg, #ff9000, #ff5000);
  -webkit-background-clip: text;
}

.item {
  display: flex;
  widows: 100%;
  margin: 0.5rem 0;
  justify-content: space-between;
}

.item>image {
  background-color: transparent;
  border-radius: 5pt;
}

.price {
  flex-grow: 1;
  display: flex;
  flex-direction: column;
  padding-left: 0.5rem;
  justify-content: space-around;
  font-size: smaller;
  width: calc(0.52*750rpx);
}

.price>text:first-child {
  /* font-size: 0.9rem; */
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.num {
  padding: 0 0.25rem;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  color: #a16b47;
  font-size: 13pt;
}

.num>text:last-child {
  font-size: 11pt;
  color: silver;
  text-align: right;
}

.list {
  display: flex;
  flex-direction: column;
}

.loan {
  display: flex;
  justify-content: flex-end;
  align-items: flex-end;
}

.loan text {
  margin: 0.5rem;
}

.orderinfo {
  display: flex;
  flex-direction: column;
  padding: 16px;
}

.infoitem {
  display: flex;
  align-items: center;
  margin: 8px 0;
  font-size: 13pt;
}

.infoitem text:first-child {
  width: 33%;
}

.infoitem text:last-child {
  flex-grow: 1;
}

.contact {
  display: flex;
  padding-top: 1rem;
  border-top: solid 1px #f6f6f6;
}

.contact image {
  height: 25px;
  width: 25px;
}

.contact view {
  flex-grow: 1;
  display: flex;
  align-items: center;
  justify-content: center;
}

.end {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 16px 0;
  color: silver;
}

.end>text:first-child {
  font-size: 1.5rem;
  font-family: little2;
}

.end>text:last-child {
  font-size: 0.9rem;
  font-family: little;
}
