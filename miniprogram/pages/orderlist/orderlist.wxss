.main {
  display: flex;
  flex-direction: column;
  padding: 0 1rem;
  align-items: flex-start;
  margin: 1rem 0;
}

.title {
  font-size: 30pt;
  align-self: stretch;
}

.title::after {
  display: inline-block;
  content: '';
  height: 8px;
  width: 8px;
  border: solid 2px #a16b47;
  border-radius: 8px;
  position: relative;
  bottom: 35px;
  right: -5px;
}

.filter {
  margin: 0 0.5rem;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 13pt;
}

.filter view {
  padding: 0.25rem;
  text-align: center;
}

.serinum {
  display: flex;
  justify-content: flex-start;
}

.serinum text {
  margin: 0.5rem ;
}

